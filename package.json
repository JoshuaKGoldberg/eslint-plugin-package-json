{
	"name": "eslint-plugin-package-json",
	"version": "0.85.0",
	"description": "Rules for consistent, readable, and valid package.json files. ðŸ—‚ï¸",
	"homepage": "https://github.com/JoshuaKGoldberg/eslint-plugin-package-json#readme",
	"bugs": {
		"url": "https://github.com/JoshuaKGoldberg/eslint-plugin-package-json/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JoshuaKGoldberg/eslint-plugin-package-json.git"
	},
	"license": "MIT",
	"contributors": [
		{
			"name": "James Zetlen",
			"email": "zetlen@gmail.com"
		},
		{
			"name": "JoshuaKGoldberg",
			"email": "npm@joshuakgoldberg.com",
			"url": "https://joshuakgoldberg.com"
		},
		{
			"name": "michael faith",
			"email": "michaelfaith@users.noreply.github.com",
			"url": "https://michael.faith"
		}
	],
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./lib/index.d.mts",
			"default": "./lib/index.mjs"
		}
	},
	"main": "lib/index.mjs",
	"files": [
		"CHANGELOG.md",
		"lib/"
	],
	"scripts": {
		"build": "tsdown",
		"build:docs": "pnpm build --no-dts && eslint-doc-generator",
		"format": "prettier .",
		"lint": "eslint . --max-warnings 0",
		"lint:docs": "eslint-doc-generator --check",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"prepare": "husky",
		"test": "vitest",
		"typecheck": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"dependencies": {
		"@altano/repository-tools": "^2.0.1",
		"change-case": "^5.4.4",
		"detect-indent": "^7.0.2",
		"detect-newline": "^4.0.1",
		"eslint-fix-utils": "~0.4.0",
		"package-json-validator": "~0.59.0",
		"semver": "^7.7.3",
		"sort-object-keys": "^2.0.0",
		"sort-package-json": "^3.4.0",
		"validate-npm-package-name": "^7.0.0"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
		"@eslint/js": "9.39.1",
		"@release-it/conventional-changelog": "10.0.2",
		"@types/estree": "1.0.8",
		"@types/node": "24.10.1",
		"@types/semver": "7.7.1",
		"@types/validate-npm-package-name": "4.0.2",
		"@vitest/coverage-v8": "4.0.13",
		"@vitest/eslint-plugin": "1.5.0",
		"console-fail-test": "0.5.0",
		"eslint": "9.39.1",
		"eslint-doc-generator": "2.3.0",
		"eslint-plugin-eslint-plugin": "7.2.0",
		"eslint-plugin-jsdoc": "61.4.1",
		"eslint-plugin-jsonc": "2.21.0",
		"eslint-plugin-n": "17.23.1",
		"eslint-plugin-perfectionist": "4.15.1",
		"eslint-plugin-regexp": "2.10.0",
		"eslint-plugin-yml": "1.19.0",
		"husky": "9.1.7",
		"jiti": "2.6.1",
		"json-schema-to-ts": "3.1.1",
		"jsonc-eslint-parser": "2.4.1",
		"knip": "5.70.2",
		"lint-staged": "16.2.7",
		"markdownlint": "0.39.0",
		"markdownlint-cli": "0.46.0",
		"prettier": "3.7.0",
		"prettier-plugin-curly": "0.4.0",
		"prettier-plugin-packagejson": "2.5.19",
		"prettier-plugin-sh": "0.18.0",
		"release-it": "19.0.6",
		"sentences-per-line": "0.3.0",
		"tsdown": "0.16.6",
		"typescript": "5.9.3",
		"typescript-eslint": "8.48.0",
		"vitest": "4.0.13"
	},
	"peerDependencies": {
		"eslint": ">=8.0.0",
		"jsonc-eslint-parser": "^2.0.0"
	},
	"packageManager": "pnpm@10.23.0",
	"engines": {
		"node": "^20.19.0 || >=22.12.0"
	},
	"publishConfig": {
		"provenance": true
	}
}
