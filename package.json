		"@vitest/eslint-plugin": "1.6.1",
		"console-fail-test": "0.6.0",
		"eslint": "10.0.0",
		"eslint-doc-generator": "3.1.0",
		"eslint-plugin-eslint-plugin": "7.3.1",
		"eslint-plugin-jsdoc": "62.5.5",
		"eslint-plugin-jsonc": "2.21.1",
		"eslint-plugin-markdown-links": "0.7.1",
