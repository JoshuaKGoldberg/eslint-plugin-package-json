		"typescript": "5.9.3",
		"typescript-eslint": "8.56.0",
		"vitest": "4.0.13"
	},
	"peerDependencies": {
		"eslint": ">=8.0.0",
		"jsonc-eslint-parser": "^2.0.0"
	},
	"packageManager": "pnpm@10.30.0",
	"engines": {
		"node": "^22.13.0 || >=24"
